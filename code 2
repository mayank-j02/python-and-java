x = input("Enter any number:")
try:
    a = int(x)
except:
    print('Invalid input')
print('the entered number is,',a)
y = input('Enter any character:')
print(y)
z = input('Enter any decimal number:')
try:
    b = float(z)
except:
    print('invalid input')
print('the entered decimal number is,',b)
